 .timescale 0 0;
    .port_info 0 /INPUT 1 "clock";
    .port_info 1 /OUTPUT 3 "light";
P_0x145f04360 .param/l "GREEN" 0 3 7, C4<010>;
P_0x145f043a0 .param/l "RED" 0 3 7, C4<100>;
P_0x145f043e0 .param/l "S0" 0 3 6, +C4<00000000000000000000000000000000>;
P_0x145f04420 .param/l "S1" 0 3 6, +C4<00000000000000000000000000000001>;
P_0x145f04460 .param/l "S2" 0 3 6, +C4<00000000000000000000000000000010>;
P_0x145f044a0 .param/l "YELLOW" 0 3 7, C4<001>;
v0x600003838000_0 .net "clock", 0 0, v0x6000038381b0_0;  1 drivers
v0x600003838090_0 .var "light", 2 0;
v0x600003838120_0 .var "state", 0 1;
E_0x600001f3c180 .event posedge, v0x600003838000_0;
    .scope S_0x145f041f0;
T_0 ;
    %wait E_0x600001f3c180;
    %load/vec4 v0x600003838120_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %pushi/vec4 4, 0, 3;
    %assign/vec4 v0x600003838090_0, 0;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x600003838120_0, 0;
    %jmp T_0.4;
T_0.0 ;
    %pushi/vec4 2, 0, 3;
    %assign/vec4 v0x600003838090_0, 0;
    %pushi/vec4 1, 0, 2;
    %assign/vec4 v0x600003838120_0, 0;
    %jmp T_0.4;
T_0.1 ;
    %pushi/vec4 1, 0, 3;
    %assign/vec4 v0x600003838090_0, 0;
    %pushi/vec4 2, 0, 2;
    %assign/vec4 v0x600003838120_0, 0;
    %jmp T_0.4;
T_0.2 ;
    %pushi/vec4 4, 0, 3;
    %assign/vec4 v0x600003838090_0, 0;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x600003838120_0, 0;
    %jmp T_0.4;
T_0.4 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0;
    .scope S_0x145f04080;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x6000038381b0_0;
    %inv;
    %store/vec4 v0x6000038381b0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x145f04080;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6000038381b0_0, 0, 1;
    %delay 100, 0;
    %vpi_call 2 13 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x145f04080;
T_3 ;
    %vpi_call 2 18 "$dumpfile", "cyclic_lamp.vcd" {0 0 0};
    %vpi_call 2 19 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x145f04080 {0 0 0};
    %vpi_call 2 20 "$monitor", $time, "RGY:%b", v0x600003838240_0 {0 0 0};
    %end;
    .thread T_3;
